@model VMPracticeViewModel
@{
    ViewData["Title"] = Model.Title;
}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@Model.Title</title>
</head>
<body>
    <h1>@Model.Header</h1>
    <div>
        PersonId : @Model.Person.PersonID
    </div>
    <div>
        Name : @Model.Person.Name
    </div>
    <div>
        Age : @Model.Person.Age
    </div>
    <div>
        Salary : @Model.Person.Salary
    </div>
    
    <h1>Person Address</h1>
    <div>
        City : @Model.Address.State
    </div>
    <div>
        State : @Model.Address.Country
    </div>    
</body>
</html>

